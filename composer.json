{
    "name": "laracraft-tech/laravel-spyglass",
    "description": "Easy XHProf setup to profile your laravel application!",
    "license": "MIT",
    "authors": [
        {
            "name": "Zacharias Creutznacher",
            "email": "zacharias@laracraft.tech",
            "homepage": "https://laracraft.tech"
        }
    ],
    "homepage": "https://github.com/laracraft-tech/laravel-spyglass",
    "keywords": ["laravel", "xhprof", "profiling", "profiler", "profile", "php"],
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "laravel/framework": "^8.37|^9.0",
        "symfony/var-dumper": "^5.0|^6.0",
        "laracraft-tech/laravel-useful-traits": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "LaracraftTech\\LaravelSpyglass\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaracraftTech\\LaravelSpyglass\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaracraftTech\\LaravelSpyglass\\SpyglassServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
